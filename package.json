{
  "name": "rntemplate",
  "version": "0.0.1",
  "description": "Elevate your mobile app development with our cutting-edge React Native template. Harness the power of the latest technologies to create stunning, high-performance apps with an exceptional user experience. Streamline your workflow and accelerate your development journey with our feature-rich, developer-friendly boilerplate.",
  "keywords": [
    "react-native",
    "template",
    "boilerplate",
    "modern",
    "sleek",
    "developer-friendly",
    "performance-optimized",
    "feature-rich"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "yarn android --mode devdebug",
    "android:prod": "yarn android --mode proddebug",
    "android:staging": "yarn android --mode stagingdebug",
    "codepush:android:dev": "appcenter codepush release-react -a iamsrikanthnani/rntemplate-1 -d Dev",
    "codepush:android:prod": "appcenter codepush release-react -a iamsrikanthnani/rntemplate-1 -d Production",
    "codepush:android:staging": "appcenter codepush release-react -a iamsrikanthnani/rntemplate-1 -d Staging",
    "ios": "react-native run-ios",
    "ios:dev": "yarn ios --scheme rntemplateDev",
    "ios:prod": "yarn ios --scheme rntemplate",
    "ios:staging": "yarn ios --scheme rntemplateStaging",
    "codepush:ios:dev": "appcenter codepush release-react -a iamsrikanthnani/rntemplate -d Dev",
    "codepush:ios:prod": "appcenter codepush release-react -a iamsrikanthnani/rntemplate -d Production",
    "codepush:ios:staging": "appcenter codepush release-react -a iamsrikanthnani/rntemplate -d Staging",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "appcenter": "^5.0.0",
    "appcenter-analytics": "^5.0.0",
    "appcenter-crashes": "^5.0.0",
    "mixpanel-react-native": "^2.3.1",
    "react": "18.2.0",
    "react-native": "0.71.13",
    "react-native-code-push": "^8.1.0",
    "react-native-config": "^1.5.1",
    "react-native-keychain": "^8.1.2",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
